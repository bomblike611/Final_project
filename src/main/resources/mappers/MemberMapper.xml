<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="MemberMapper">
  	<insert id="memberJoin" parameterType="SingerDTO">
  		insert into 
  		member values(#{id}, #{pw}, #{name}, #{age}, #{email}, #{phone}, #{addr}, #{birth}, #{job}, 0, #{fname}, #{oname})
  	</insert>
  	
  	<insert id="singerJoin" parameterType="SingerDTO">
  		insert into
  		singer values(#{id}, #{teamName}, #{position}, 0, #{sns}, 0)
  	</insert>
  
 
  
  </mapper>