<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="CommendMapper">
  	<insert id="recommend" parameterType="com.hj.recommend.recommendDTO">
  		<selectKey resultType="int" order="BEFORE" keyProperty="num">
  			select board_seq.nextval from dual
  		</selectKey>
  			insert into recommend values(#{num}, #{title}, #{writer},#{contents}, sysdate, 0)
  	</insert>
  </mapper>